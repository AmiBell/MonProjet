        android:textStyle="bold" />

    <EditText
        android:id="@+id/editText39"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="time"
        android:text="13:20"
        android:textStyle="bold"
        android:layout_above="@+id/editText24"
        android:layout_alignRight="@+id/editText19"
        android:layout_alignEnd="@+id/editText19" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/multiAutoCompleteTextView"
        android:layout_alignEnd="@+id/button12"
        android:layout_alignRight="@+id/button12" />


</RelativeLayout>
